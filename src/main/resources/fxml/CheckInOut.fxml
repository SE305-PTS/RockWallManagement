<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>
<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="PTS.CheckInOutController">
   <children>
      <BorderPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
         <top>
            <MenuBar BorderPane.alignment="CENTER">
              <menus>
                <Menu mnemonicParsing="false" text="Rock Wall Management">
                  <items>
                    <MenuItem fx:id="checkInOutExit" mnemonicParsing="false" text="Return to Main Page" />
                  </items>
                </Menu>
              </menus>
            </MenuBar>
         </top>
         <center>
            <VBox alignment="CENTER" spacing="12.0" BorderPane.alignment="CENTER">
               <children>
                  <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Rock Wall Check In/Out">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
                  <Text fx:id="checkInSuspendedText" fill="RED" strokeType="OUTSIDE" strokeWidth="0.0" text="This patron is currently suspended" />
                  <HBox alignment="CENTER">
                     <children>
                        <GridPane maxWidth="-Infinity" minWidth="-Infinity" prefWidth="350.0">
                          <columnConstraints>
                            <ColumnConstraints halignment="RIGHT" hgrow="SOMETIMES" maxWidth="369.0" minWidth="10.0" percentWidth="28.0" prefWidth="85.0" />
                            <ColumnConstraints hgrow="SOMETIMES" maxWidth="693.0" minWidth="10.0" prefWidth="688.0" />
                          </columnConstraints>
                          <rowConstraints>
                            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                          </rowConstraints>
                           <VBox.margin>
                              <Insets />
                           </VBox.margin>
                           <children>
                              <HBox alignment="CENTER" prefHeight="30.0" prefWidth="268.0">
                                 <children>
                                    <Text strokeType="OUTSIDE" strokeWidth="0.0" text="ID Number" textAlignment="RIGHT" />
                                 </children>
                              </HBox>
                              <TextField fx:id="checkInOutID" GridPane.columnIndex="1" />
                           </children>
                        </GridPane>
                     </children>
                  </HBox>
                  <Button fx:id="checkInOutSubmit" defaultButton="true" mnemonicParsing="false" text="Submit">
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </Button>
                  <Text fx:id="checkInSuccessText" strokeType="OUTSIDE" strokeWidth="0.0" text="Patron with ID (id number) has been checked (in/out)" />
               </children>
               <BorderPane.margin>
                  <Insets />
               </BorderPane.margin>
            </VBox>
         </center>
      </BorderPane>
   </children>
</AnchorPane>
